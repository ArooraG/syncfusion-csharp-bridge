<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <!-- Stable version .NET 8.0 par set kiya gaya -->
    <TargetFramework>net8.0</TargetFramework> 
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" />
    
    <!-- Zaroori Syncfusion Packages -->
    <PackageReference Include="Syncfusion.Pdf.NET" Version="31.2.3" /> 
    <PackageReference Include="Syncfusion.DocIO.NET" Version="31.2.3" /> 
    <PackageReference Include="Syncfusion.XlsIO.NET" Version="31.2.3" /> 
    
    <!-- FIX: PDF to Word/Excel ke liye naye packages -->
    <PackageReference Include="Syncfusion.PdfToWordConverter.NET" Version="31.2.3" />
    <PackageReference Include="Syncfusion.PdfToExcelConverter.NET" Version="31.2.3" />
  </ItemGroup>

</Project>